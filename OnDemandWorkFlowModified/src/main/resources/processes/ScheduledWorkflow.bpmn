    </sequenceFlow>
    <serviceTask id="servicetask4" name="Change package status to &quot;Cancelled&quot;" activiti:delegateExpression="${changePackageStatusDelegate}"></serviceTask>
    <sequenceFlow id="flow5" sourceRef="servicetask3" targetRef="servicetask4"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow6" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
